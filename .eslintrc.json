{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off",
    "no-param-reassign": "off"
  }
}
